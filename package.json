{
  "name": "imgui-docs-formatter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "24.0.23",
    "@types/lodash": "^4.14.148",
    "@types/node": "12.12.8",
    "@types/uuid": "^3.4.6",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "3.7",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "convert": "ts-node -P tsconfig.json convert.ts",
    "convert-dev": "ts-node-dev -P tsconfig.json --respawn --transpileOnly convert.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
